<div class="container">
<div>
<%= simple_form_for @cocktail do |f| %>
  <%= f.error_notification %>

  <%= f.input :name, placeholder: "Cocktail name" %>
  <%= f.input :picture %>

  <%= f.button :submit, class: "btn btn-primary" %>
<% end %>
</div>
<p>
  <%= link_to "Back", root_path, class: "btn btn-info btn-back" %>
</p>
</div>
